    "devDependencies": {
        "@parcel/transformer-glsl": "^2.12.0",
        "express": "^4.19.2",
        "parcel": "^2.12.0",
        "parcel-reporter-clean-dist": "^1.0.4",
        "parcel-reporter-static-files-copy": "^1.5.3",
        "process": "^0.11.10"
    },
    "dependencies": {
        "phaser": "^3.80.1"
    },
    "staticFiles": {
        "staticPath": "public"
